{
    "version": "0.2.0",
    "configurations": [
        {   // Python Debugger: Current File
            "name":"Python Debugger: Current File",
            "type":"debugpy",
            "request":"launch",
            "program":"${file}",
            "console":"integratedTerminal",
            "justMyCode": true
        },
        {   // Python Debugger: Current File with Args
            "name":"Python Debugger: Current File with Args",
            "type":"debugpy",
            "request":"launch",
            "program":"${file}",
            "console":"integratedTerminal",
            "args": [
                "--input", 
                "${workspaceFolder}/TEST_AREA",
                "--output",
                "tosec_2012.duckdb",
                "--workers", 
                "1"
            ]
        },
        {   // Debug Tests (Pytest - Current File)
            "name": "Debug Tests (Pytest - Current File)",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-s",           // print() çıktılarını görmek için
                "-v",           // Detaylı çıktı için (verbose)
                "${file}"       // O an açık olan dosya
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
            "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        { // Debug: Turbo-TOSEC Staged (v2012-04-23)
            "name": "Debug: Turbo-TOSEC Staged (v2012-04-23)",
            "type": "debugpy",
            "request": "launch",
            "module": "turbo_tosec.cli", 
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "scan",
                "--input", 
                "E:/HOME/RetroVault/TOSEC_DATs/Extracted/TOSEC-v2012-04-23",
                "--output",
                "${workspaceFolder}/TEST_AREA/v2012-04-23.duckdb",
                "--staged", 
                "--workers", 
                "4",
                "--temp-dir",
                "debug_temp_chunks"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        { // Debug: Turbo-TOSEC Staged (v2025-03-13)
            "name": "Debug: Turbo-TOSEC Staged (v2025-03-13)",
            "type": "debugpy",
            "request": "launch",
