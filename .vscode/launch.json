            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "scan",
                "--input", 
                "E:/HOME/RetroVault/TOSEC_DATs/Extracted/TOSEC-v2025-03-13",
                "--output",
                "E:/HOME/Documents/Databases/turbo-tosec/duckdb/tosec-v2025-03-13.duckdb",
                "--staged", 
                "--workers", 
                "4",
                "--temp-dir",
                "debug_temp_chunks"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        { // Debug: Turbo-TOSEC Direct (v2016-11-11)
            "name": "Debug: Turbo-TOSEC Direct (v2016-11-11)",
            "type": "debugpy",
            "request": "launch",
            "module": "turbo_tosec.cli",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "scan",
                "--input", 
                "E:/HOME/RetroVault/TOSEC_DATs/Extracted/TOSEC-v2016-11-11",
                "--output",
                "${workspaceFolder}/TEST_AREA/v2016-11-11.duckdb",
                "--direct"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        { // Debug: Turbo-TOSEC Direct (v2012-04-23)
            "name": "Debug: Turbo-TOSEC Direct (v2016-11-11)",
            "type": "debugpy",
            "request": "launch",
            "module": "turbo_tosec.cli",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "scan",
